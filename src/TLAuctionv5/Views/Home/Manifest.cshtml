@using TLAuctionv5.ViewModels.Main;
@model MainViewModel


<div id="content">
    <div id="contentbody">
        <div class="psdgraphics-com-table">
            <div class="psdg-header">
                <span class="psdg-bold">@Html.DisplayFor(model => model.Manifests.FirstOrDefault().AuctionTitle)</span><br />
            </div>
            <div class="psdg-top">
                <div class="psdg-top-cell" style="width:50px;">Qty</div>
                <div class="psdg-top-cell" style="width:90px;">Brand</div>
                <div class="psdg-top-cell" style="width:125px;">Part #</div>
                <div class="psdg-top-cell" style="width:258px;padding: 11px 0 0 2px;">Item Title</div>
                <div class="psdg-top-cell" style="width:90px;">UPC</div>
                <div class="psdg-top-cell" style="width:90px;">SKU</div>
                <div class="psdg-top-cell" style="width:90px;">Avg Price</div>
                <div class="psdg-top-cell" style="width:90px;">Total</div>
                <div class="psdg-top-cell" style="width:60px;">Prod Cnt</div>
            </div>
            <div class="psdg-middle">
                @foreach (ManifestOpenView manifest in Model.Manifests)
                {
                    <div class="tl-manifest-result-wrap clearfix">
                        <div class="psdg-right" style="width:50px;">@manifest.Quantity</div>
                        <div class="psdg-right" style="width:90px;">@manifest.Manufacturer</div>
                        <div class="psdg-right" style="width:125px;">@manifest.Partno</div>
                        <div class="psdg-right" style="width:258px; text-align: left; padding: 11px 0 0 2px;">@manifest.Title</div>
                        <div class="psdg-right" style="width:90px;">@manifest.UPC</div>
                        <div class="psdg-right" style="width:90px;">@manifest.Sku</div>
                        <div class="psdg-right" style="width:90px;">@manifest.AvgPrice</div>
                        <div class="psdg-right" style="width:90px;">@manifest.Total</div>
                        <div class="psdg-right" style="width:60px;">@manifest.ProductCnt</div>
                    </div>
                }
            </div>
        </div>

    </div>
</div>




